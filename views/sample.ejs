
<style>
    table, td, th {
        border: 1px solid black;
    }
    
    table {
        border-collapse: collapse;
        width: 100%;
    }
</style>

<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email Address</th>
        <th>Gender</th>
        <th>Date of Birth</th>
        <th>Height</th>
        <th>Weight</th>
        <th>Ethenticity</th>
        <th>Telephone Number</th></t></th>
        <th>Address</th>
        <th>Postal Code</th>
        <th>City</th>
        <th>State Province</th>
        <th>Country</th>
    </tr>
    <% patients.forEach(function(patient) { %>
        <tr>
            <td><%= patient.id %></td>
            <td><%= patient.name %></td>
            <td><%= patient.email %></td>
            <td><%= patient.gender %></td>
            <td><%= patient.date_of_birth %></td>
            <td><%= patient.height %></td>
            <td><%= patient.weight %></td>
            <td><%= patient.ethenticity %></td>
            <td><%= patient.tel %></td>
            <td><%= patient.address %></td>
            <td><%= patient.postal_code %></td>
            <td><%= patient.city %></td>
            <td><%= patient.state_province %></td>
            <td><%= patient.country %></td>
        </tr>
    <% }); %>
</table>